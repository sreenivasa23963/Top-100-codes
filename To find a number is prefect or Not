#To find a number is prefect or Not
def perfect_number(num):
    if num<=0:
        return False
    sum_divisor=0
    for i in range(1,num):
        if num%i==0:
            sum_divisor +=i
    if sum_divisor==num:
        print("Perfect NUmber")
    else:
        print("Not A perfect number")

num=int(input())
perfect_number(num)
